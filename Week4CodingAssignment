function concatenateWord(word, n) {
  let concatenatedWord = '';
  for (let i = 0; i < n; i++) {
    concatenatedWord += word;
  }
  return concatenatedWord;
}


function fullName(firstName, lastName) {
  let fullName = firstName + ' ' + lastName;
  return fullName;
}


function sumGreaterThanZero(numbers) {
  let sum = 0;
  for (let i = 0; i < numbers.length; i++) {
    sum += numbers[i];
  }
  return sum > 100;
}

function calculateAverage(numbers) {
    if (numbers.length === 0) {
      return 0;
    }
   
    function isAverageGreaterThan(numbers1, numbers2) {
        const average1 = calculateAverage(numbers1);
        const average2 = calculateAverage(numbers2);
        return average1 > average2;
      }
      
  function calculateAverage(numbers) {
        if (numbers.length === 0) {
          return 0;
        }


function willBuyDrink(isHotOutside, moneyInPocket) {
  return isHotOutside && moneyInPocket > 10.50;
}
  }
}

function calculateScore(touchdowns, fieldGoals, extraPoints) {
    const touchdownScore = touchdowns * 6;
    const fieldGoalScore = fieldGoals * 3;
    const extraPointScore = extraPoints * 1;
    return touchdownScore + fieldGoalScore + extraPointScore;
  }
This function was made to set the number of points you get for scoring in various ways in football, I'm a huge football fan and thought it would be something fun to test out. 